public class DigitCount {
    public static void main(String[] args) {
        int n = 1234;
        int count = 0;  
        while (n > 0) { 
            n = n / 10; //Dividing the numeber by 10 
            count++; //increment the count value
        }
        System.out.println("Number of digits: " + count);
    }
}
